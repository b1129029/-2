#include <iostream>
#include <ctime>
using namespace std; 

int main() {
    srand(time(0));
	int sort_times = 32766;
    
    int* arr0=new int[sort_times+1];
	
    for(int i=1; i<=sort_times; i++) {
        arr0[i]=rand()%50000+1;
    }
	
    cout << "排序前的數列：";
    for(int i=1; i<=sort_times; i++) {
    	cout<<arr0[i]<<" ";
	}
	cout << endl;
    return 0;
}    
